{
  "name": "www",
  "version": "0.0.223",
  "module": "src/pages/index.tsx",
  "type": "module",
  "scripts": {
    "dev": "brisa dev",
    "dev:debug": "brisa dev --debug",
    "build:json": "bun run src/scripts/docs-to-json.ts",
    "build:editor": "cp ../../node_modules/monaco-editor/min/vs/language/typescript/tsWorker.js ../../node_modules/monaco-editor/min/vs/editor/editor.main.js src/public/monaco-editor/",
    "build": "bun run build:editor && bun run build:json && brisa build",
    "start": "brisa start"
  },
  "dependencies": {
    "brisa": "workspace:*",
    "brisa-adapter-vercel": "workspace:*",
    "gray-matter": "4.0.3",
    "mark.js": "8.11.1",
    "markdown-it": "14.1.0",
    "markdown-it-container": "4.0.0",
    "markdown-it-shikiji": "0.10.2",
    "minisearch": "7.1.0",
    "search-engine-wc": "0.2.2",
    "shikiji-transformers": "0.10.2",
    "slugify": "1.6.6",
    "@swc/wasm-web": "1.7.28",
    "monaco-editor": "0.52.0"
  },
  "devDependencies": {
    "@types/bun": "1.1.10",
    "@types/markdown-it": "14.1.2",
    "@types/markdown-it-container": "2.0.10",
    "@types/mark.js": "8.11.12",
    "jsdom": "25.0.1",
    "typescript": "5.6.2",
    "@types/jsdom": "21.1.7"
  }
}
