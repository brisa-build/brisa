{
  "name": "www",
  "version": "0.1.2",
  "module": "src/pages/index.tsx",
  "type": "module",
  "scripts": {
    "dev": "brisa dev",
    "dev:debug": "brisa dev --debug",
    "build:json": "bun run src/scripts/docs-to-json.ts",
    "build:og-images": "bun run src/scripts/og-images.tsx",
    "build:rss": "bun run src/scripts/generate-rss.ts",
    "build": "bun run build:json && brisa build && bun run build:og-images && bun run build:rss",
    "start": "brisa start"
  },
  "dependencies": {
    "brisa": "workspace:*",
    "brisa-adapter-vercel": "workspace:*",
    "gray-matter": "4.0.3",
    "mark.js": "8.11.1",
    "markdown-it": "14.1.0",
    "markdown-it-container": "4.0.0",
    "markdown-it-shikiji": "0.10.2",
    "minisearch": "7.1.0",
    "search-engine-wc": "0.2.3",
    "shikiji-transformers": "0.10.2",
    "slugify": "1.6.6",
    "@swc/wasm-web": "1.7.36"
  },
  "devDependencies": {
    "@types/bun": "1.1.11",
    "@types/markdown-it": "14.1.2",
    "@types/markdown-it-container": "2.0.10",
    "@types/mark.js": "8.11.12",
    "jsdom": "25.0.1",
    "typescript": "5.6.3",
    "@types/jsdom": "21.1.7",
    "sharp": "0.33.5"
  },
  "packageManager": "bun@1.1.31",
  "engines": {
    "bun": ">= 1.1.31",
    "npm": "please-use-bun",
    "yarn": "please-use-bun",
    "pnpm": "please-use-bun"
  }
}
